{
  "id": "planningdragdrop",
  "version": "1.0.8",
  "name": "Planning Drag Drop Panel",
  "newInstancePrefix": "PDD",
  "description": "Professional Dual Panel Drag Drop for SAC Planning",

  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-planning-dragdrop",
      "url": "https://sacplanning2025.github.io/dimension-layout-widget/main.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],

  "properties": {
    "availableItems": {
      "type": "string",
      "default": ""
    },
    "selectedItems": {
      "type": "string",
      "default": ""
    }
  },

  "events": {
  "onLayoutChanged": {
    "description": "Triggered when layout changes"
    }
  },

 "methods": {
  "setAvailable": {
    "parameters": [
      {
        "name": "items",
        "type": "string[]"
      }
    ]
  },
  "getRows": {
    "returnType": "string[]"
  },
  "getColumns": {
    "returnType": "string[]"
  },
  "getAvailable": {
    "returnType": "string[]"
  }
},
"events": {
  "onLayoutChanged": {}
}
}
